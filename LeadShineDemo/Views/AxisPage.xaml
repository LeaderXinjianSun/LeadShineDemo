                                </i:EventTrigger>
                                <i:EventTrigger EventName="PreviewMouseUp">
                                    <i:InvokeCommandAction Command="{Binding Axis_Jog_Stop_Command}" CommandParameter="1"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
